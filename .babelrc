{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "legacy": true
      }
    ],
    ["import", {
      "libraryName": "antd",
      "style": "css"
    }],
    [
      "transform-es2015-modules-commonjs",
      {
        "legacy": true
      }
    ]
  ]
}